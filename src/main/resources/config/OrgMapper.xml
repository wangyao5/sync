<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kingdee.letv.sync.mapper.OrgMapper">
	<resultMap id="OrgMapper" type="com.kingdee.letv.sync.model.OrgModel" >
		<id column="Fid" property="id" jdbcType="INTEGER" />
	    <result column="FOrg_num" property="org_num" jdbcType="VARCHAR" />
	    <result column="FOrg_name" property="org_name" jdbcType="VARCHAR" />
	</resultMap>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="com.kingdee.letv.sync.model.OrgModel">
		insert into T_ORG_ADMIN(Forg_num,Forg_name,Forg_pnum)values(#{org_num},#{org_name},#{pNum})
	</insert>
	
	<select id="selectByNum" parameterType="STRING" resultMap="OrgMapper">
		select * from T_ORG_ADMIN where Forg_num=#{org_num}  
	</select>
	<select id="selectAll" resultMap="OrgMapper">
		select * from T_ORG_ADMIN
	</select>
	<update id="updateOrgModel"  parameterType="com.kingdee.letv.sync.model.OrgModel">
		update T_ORG_ADMIN set Forg_name=#{org_name},Forg_pnum=#{pNum} where Forg_num=#{org_num}
	</update>
</mapper>