<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kingdee.letv.sync.mapper.LogMapper">
	<resultMap id="logMap" type="com.kingdee.letv.sync.model.LogModel" >
		<id column="FID" property="id" jdbcType="INTEGER" />
	    <result column="FSynTime" property="synTime" jdbcType="VARCHAR" />
	    <result column="FIntNo" property="intNo" jdbcType="VARCHAR" />
	    <result column="FStatus" property="status" jdbcType="INTEGER" />
	    <result column="FParams" property="params" jdbcType="VARCHAR" />
	    <result column="FResult" property="result" jdbcType="VARCHAR" />
	    <result column="FMidParams" property="midParams" jdbcType="VARCHAR" />
	    <result column="FTotalCount" property="totalCount" jdbcType="INTEGER" />
	    <result column="FSucessCount" property="successCount" jdbcType="INTEGER" />
	</resultMap>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="com.kingdee.letv.sync.model.LogModel">
		insert into T_MCL_Log(FSynTime,FIntNo,FStatus,FParams,FResult,FMidParams,FTotalCount,FSucessCount)
		values(#{synTime},#{intNo},#{status},#{params},#{result},#{midParams},#{totalCount},#{successCount})
	</insert>
	
	<select id="selectById" parameterType="STRING" resultMap="logMap">
		select fid, FSynTime,FParams,FResult from T_MCL_Log where FintNo=#{id} order by FSynTime DESC
	</select>
	
	<select id="selectByLimit"  resultMap="logMap">
		select fid, FSynTime,FParams,FResult from T_MCL_Log where FintNo=#{0} order by FSynTime DESC LIMIT #{1},#{2} 
	</select>
	
	<select id="selectAll" resultMap="logMap">
		select  fid, FSynTime,FParams,FResult from T_MCL_Log
	</select>
	
	<select id="selectByFilter" resultMap="logMap">
		select fid,    FSynTime,FParams,FResult from T_MCL_Log where 1=1 and #{filter} order by FSynTime DESC
	</select>	
</mapper>